  "jest": {
    "setupFilesAfterEnv": [
      "@testing-library/jest-dom"
    ],
    "testPathIgnorePatterns": [
      "<rootDir>/.next/",
      "<rootDir>/node_modules/"
    ],
    "transform": {
      "^.+\\.(js|jsx|ts|tsx)$": "ts-jest"
