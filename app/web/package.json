    "@mui/x-data-grid": "^5.13.1",
    "@mui/x-date-pickers": "^5.0.0-beta.0",
    "@visx/group": "^3.3.0",
    "@visx/scale": "^3.5.0",
    "@visx/shape": "^3.5.0",
    "@visx/text": "^3.3.0",
    "date-fns": "^2.28.0",
    "next": "^12.2.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-hook-form": "^7.44.3"
  },
  "devDependencies": {
    "@faker-js/faker": "^8.4.1",
